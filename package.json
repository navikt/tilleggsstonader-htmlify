{
  "name": "tilleggsstonader-htmlify",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --es-module-specifier-resolution=node dist/src/server/index.js",
    "build": "tsc -p tsconfig.json",
    "start:dev": "nodemon -e ts,tsx --exec \"yarn build:dev && yarn run:dev\"",
    "build:dev": "tsc -p tsconfig.json",
    "run:dev": "node --es-module-specifier-resolution=node dist/src/server/index.js",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.11.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.2",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winston": "^3.10.0"
  }
}
